.site-home {
  background-image: url("/images/mastheads/millmint.png");
  background-position: 50% 50px;
  background-repeat: no-repeat;
  background-attachment: fixed;
  background-color: var(--body-background);
  background-size: cover; }

.shop-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(240px, 1fr));
  gap: 10px 10px;
  grid-auto-flow: row;
  grid-auto-rows: 1fr; }

.shop-card h1, .shop-card h2, .shop-card h3, .shop-card a, .shop-card p {
  margin: 0 !important;
  color: var(--font-color); }

.shop-card h2 {
  color: var(--color-gray); }

.shop-card a {
  color: var(--font-color) !important; }

.shop-title h1 {
  font-weight: 900 !important;
  position: absolute;
  margin: 5rem; }

.card-img {
  background: var(--body-background); }

.shop-card {
  background: var(--gray-light);
  padding: 1rem;
  border-radius: 5px;
  transition: box-shadow .3s; }

.shop-card:hover {
  box-shadow: 0 0 5px rgba(33, 33, 33, 0.2); }

.shop-item {
  justify-self: center;
  align-self: center; }

.postcard-layout {
  margin-top: .5rem;
  margin-bottom: .5rem; }
  .postcard-layout li {
    margin: 0; }
  .postcard-layout .post-list {
    padding: 0; }
  .postcard-layout .post-grid {
    margin: 2.5rem 0;
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(240px, 1fr));
    gap: 10px;
    grid-auto-flow: row; }
  .postcard-layout .card {
    border-radius: .25rem;
    width: 100%;
    box-sizing: border-box;
    background: var(--gray-light);
    padding: 0;
    transition: ease 0.15s;
    position: relative; }
  .postcard-layout .post-info {
    padding: .5rem;
    /* position: absolute;
    top: 0;
    left: 0;
    z-index: 1; */ }
  .postcard-layout .post-info .text-base {
    border-bottom: 0;
    line-height: 1.5;
    padding: 5px 0; }
  .postcard-layout .post-info .post-title {
    padding: 0 5px;
    border-radius: 7px;
    box-sizing: border-box; }
  .postcard-layout .card .card-title {
    color: var(--darker-text); }
  .postcard-layout .post-title span {
    color: var(--font-color); }
  .postcard-layout .post-info .a-wrapper {
    display: block;
    padding: 0px 10px 5px 5px;
    margin-top: -5px;
    font-size: 14px;
    color: var(--darker-text); }
  .postcard-layout .card .img-wrapper {
    border-radius: .25rem;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    overflow: hidden; }
  .postcard-layout .card .img-wrapper img {
    transform: scale(1.1); }
  .postcard-layout .card ul {
    padding-inline-start: 0; }
  .postcard-layout .card:hover {
    --tw-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.1),
    0 10px 10px -5px rgba(0, 0, 0, 0.04);
    box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);
    transform: translate(0, -2.5px); }
  .postcard-layout .item {
    justify-self: center;
    align-self: center;
    list-style: none; }
  .postcard-layout .card img {
    width: 100%;
    min-height: auto !important;
    height: 360px !important;
    object-fit: cover !important;
    margin-bottom: 0;
    margin-top: 0 !important; }
  .postcard-layout .post-grid {
    list-style: none;
    padding: 0; }

.search {
  position: relative;
  justify-items: end;
  border-bottom: 1px solid transparent;
  min-width: 10rem;
  grid-area: search; }
  .search input {
    background-color: var(--highlight-background);
    width: 100%;
    height: 35px;
    padding: 2px;
    box-sizing: border-box;
    border-radius: 5px;
    color: var(--color-gray); }
    .search input:required + .search-spinner {
      display: block; }
  .search .search-spinner {
    position: absolute;
    top: 0;
    margin: 8px;
    margin-inline-start: calc(100% - 24px);
    width: 16px;
    height: 16px;
    border: 1px solid transparent;
    border-top-color: var(--font-color);
    border-radius: 50%;
    animation: spin 1s ease infinite; }

@keyframes spin {
  100% {
    transform: rotate(360deg); } }

.noSelect {
  -webkit-tap-highlight-color: transparent;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none; }

.noSelect:focus {
  outline: none !important; }

#search-results {
  background-color: var(--body-background);
  margin-top: 0.25rem;
  position: absolute;
  z-index: 11;
  width: -webkit-max-content;
  width: -moz-max-content;
  width: max-content;
  --tw-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.1),
    0 10px 10px -5px rgba(0, 0, 0, 0.04);
  box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);
  max-width: 100%;
  border-radius: 5px;
  padding-inline-start: 0; }

#search-results li a:hover {
  color: var(--color-gray);
  border-radius: 2px;
  text-decoration: underline; }

.search ul li {
  font-size: 16px;
  line-height: 1.4rem;
  padding: 0.25rem 1rem 0.25rem 1rem; }

.search ul {
  padding-top: 1rem;
  padding-bottom: 1rem; }

.search .found:not(:empty) {
  display: block;
  padding: 3px;
  padding-left: 8px;
  border-left: 1px solid var(--font-color);
  opacity: 0.5;
  font-size: 80%; }

.search mark {
  background: var(--color-gray); }

.search small {
  opacity: 0.5;
  display: block; }

.search ul li {
  list-style: none; }

