&lt;/div&gt;&lt;input type=&#34;radio&#34; class=&#34;toggle&#34; name=&#34;tabs-%!s(&amp;lt;nil&amp;gt;)&#34; id=&#34;tabs-%!s(&amp;lt;nil&amp;gt;)-2&#34;  /&gt;
  &lt;label for=&#34;tabs-%!s(&amp;lt;nil&amp;gt;)-2&#34;&gt;Paintings&lt;/label&gt;
